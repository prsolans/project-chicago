# ElevenLabs API Key for Text-to-Speech
# Get your API key from: https://elevenlabs.io/
VITE_ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# ElevenLabs Voice ID
# Default is Rachel voice (21m00Tcm4TlvDq8ikWAM)
# Get voice IDs from: https://elevenlabs.io/voice-library
VITE_ELEVENLABS_VOICE_ID=21m00Tcm4TlvDq8ikWAM

# Anthropic API Key for AI predictions (Phase 1+)
# Get your API key from: https://console.anthropic.com/
VITE_ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Prediction Cache Settings
# Maximum number of predictions to cache
VITE_PREDICTION_CACHE_SIZE=1000

# Cache time-to-live in minutes
VITE_CACHE_TTL_MINUTES=30

# Enable real-time streaming predictions (Phase 5)
VITE_ENABLE_STREAMING=true

# Supabase credentials (Phase 2)
# Get from your Supabase project settings: https://supabase.com/
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here
